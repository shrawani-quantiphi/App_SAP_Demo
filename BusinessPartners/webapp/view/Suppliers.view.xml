<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.BusinessPartners.controller.Suppliers" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="Suppliers">
					<content>
					    <List noDataText="Drop list items here" id="list1" items="{/BusinessPartnerSet}">
					        <items>
					            <StandardListItem type="Navigation" title="{CompanyName}" description="{BusinessPartnerID}" icon="sap-icon://picture" id="item2"/>
					        </items>
					    </List>
					    <List noDataText="Drop list items " id="list0">
					    </List>
					</content>
				<headerContent>
				    <sap.ushell.ui.footerbar:AddBookmarkButton text="Bookmark added!" icon="sap-icon://add-favorite" xmlns:sap.ushell.ui.footerbar="sap.ushell.ui.footerbar" id="button1"/>
				</headerContent>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>